public class 100{
public static void name(String[] args){
for(int i=1;i<=100;i++) {
			if (i%2==0 && i%3==0) System.out.println("The number " + i + " is even and divisible by 3");
			else if (i%2==0) System.out.println("The number " + i + " is even");
			else if (i%3==0) System.out.println("The number " + i + " is divisible by 3");
			}
			}
			}
